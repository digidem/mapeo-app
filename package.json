{
  "name": "dd-reporter-app",
  "version": "0.1.0",
  "description": "A reporter app made for empowerment by Digital Democracy",
  "main": "redux/index.jsx",
  "keywords": [
    "reporter",
    "i_dont_really_know"
  ],
  "author": "Digital Democracy",
  "contributors": [
    "Gregor MacLennan",
    "Tiago Fassoni"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/digidem/reporter-app.git"
  },
  "license": "MPL-2.0",
  "browserify": {
    "transform": [
      "babelify",
      "envify"
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "scripts": {
    "build": "NODE_ENV=production browserify src/index.js | uglifyjs -cm > dist/index.js",
    "build:gh-pages": "npm run build && buildbranch gh-pages dist",
    "prebuild": "mkdir -p dist && cp -Rf index.html circle.yml img dist/",
    "serve:build": "st -i index.html --dir dist",
    "lint": "standard ./src",
    "start": "NODE_ENV=development budo src/index.js:index.js -- -p livereactload | garnish",
    "start:nohot": "NODE_ENV=development budo src/index.js:index.js | garnish",
    "test": "npm run lint && npm run test:tap",
    "test:tap": "babel-tape-runner 'tests/**/*.test.js'",
    "test:browser": "babel-tape-runner 'tests/**/*.test.js' -b"
  },
  "dependencies": {
    "camelcase": "^2.0.0",
    "history": "^1.12.6",
    "id-geo": "^1.0.0-alpha1",
    "immutable": "^3.7.5",
    "immutable-js": "^0.3.1-6",
    "invariant": "^2.1.2",
    "keymirror": "^0.1.1",
    "lodash": "^3.10.1",
    "mapbox-gl": "^0.11.3",
    "material-ui": "^0.13.1",
    "raf": "^3.1.0",
    "react": "^0.14.3",
    "react-addons-pure-render-mixin": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-infinite": "^0.7.1",
    "react-intl": "^2.0.0-beta-1",
    "react-motion": "^0.3.1",
    "react-motion-ui-pack": "^0.5.1",
    "react-pure-render": "^1.0.2",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0",
    "react-tap-event-plugin": "^0.2.1",
    "redux": "^3.0.4",
    "redux-immutablejs": "0.0.6",
    "redux-simple-router": "0.0.7",
    "redux-thunk": "^1.0.0",
    "turf-distance": "^1.0.1",
    "turf-point": "^2.0.1",
    "uuid": "^2.0.1",
    "watchify": "^3.4.0"
  },
  "devDependencies": {
    "babel": "^5.8.29",
    "babel-eslint": "^4.1.3",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-tape-runner": "1.2.0",
    "babelify": "^6.3.0",
    "browserify": "^12.0.1",
    "budo": "^5.1.1",
    "buildbranch": "0.0.3",
    "d3": "^3.5.6",
    "deep-equal": "^1.0.1",
    "envify": "^3.4.0",
    "eslint": "^1.6.0",
    "eslint-config-standard": "^4.4.0",
    "eslint-config-standard-react": "^1.1.0",
    "eslint-plugin-react": "^3.9.0",
    "eslint-plugin-standard": "^1.3.1",
    "garnish": "^3.2.1",
    "livereactload": "^2.0.5",
    "prova": "^2.1.2",
    "proxyquire": "^1.7.3",
    "react-addons-test-utils": "^0.14.2",
    "react-proxy": "^1.1.1",
    "reactify": "^1.1.1",
    "redux-devtools": "^2.1.5",
    "redux-logger": "^2.0.4",
    "smash": "^0.0.15",
    "st": "^1.0.0",
    "standard": "^5.3.1",
    "turf-random": "^1.0.2",
    "uglify-js": "^2.4.24"
  }
}
