{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": ["es6"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": false,
    "target": "esnext"
  },
  "exclude": ["node_modules", ".babelrc", "metro.config.js", "jest.config.js"],
  "include": ["src/frontend/**/*"]
}
